{
  "name": "angular-cl2",
  "version": "0.2.0",
  "description": "Angular macros for ChlorineJS (a subset of Clojure)",
  "scripts": {
    "watch": "cl2c -d -p test/test.cl2 test/test.hic",
    "live-test": "testem -f test/testem.json",
    "bare-compile": "cl2c -1 -b src/main.cl2 src/index.hic",
      "compile": "cl2c -p -B src/main.cl2 src/index.hic",
      "compile-once": "cl2c -1 -B src/main.cl2 src/index.hic",
    "pretest": "cl2c -1 -d test/test.cl2 test/test.hic",
    "test": "testem  -s phantomjs -f test/testem.json ci"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/chlorinejs/angular-cl2.git"
  },
  "keywords": [
    "angular",
    "qunit",
    "chlorinejs",
    "clojure",
    "macro"
  ],
  "directories": {
    "lib":  "./lib"
  },
  "analyze": false,
  "devDependencies": {
    "chloric"  :  "0.1.11",
    "testem"   :  "*"
  },
  "license": "EPL",
  "engines": {
    "node": ">=0.8"
  }
}
