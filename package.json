{
  "name": "pricing-table",
  "version": "0.2.0",
  "description": "An AngularJS + ChlorinJS app",
  "scripts": {
    "watch": "cl2c -d -p test/test.cl2 test/test.hic",
    "live-test": "testem -f test/testem.json",
      "bare-compile": "cl2c -1 -b src/main.cl2 src/index.hic src/demo.cl2 src/demo.hic",
      "compile": "cl2c -p -B src/main.cl2 src/index.hic src/demo.cl2 src/demo.hic",
      "compile-once": "cl2c -1 -B src/main.cl2 src/index.hic src/demo.cl2 src/demo.hic",
    "pretest": "cl2c -1 -d test/test.cl2 test/test.hic",
    "test": "testem  -s phantomjs -f test/testem.json ci"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/myguidingstar/pricing-table.git"
  },
  "keywords": [
    "angular",
    "qunit",
    "chlorinejs",
    "clojure",
    "macro"
  ],
  "directories": {
    "lib":  "./lib"
  },
  "analyze": false,
  "devDependencies": {
    "angular-cl2" : "0.2.2",
    "chloric"  :  "0.1.13",
    "testem"   :  "*"
  },
  "license": "EPL",
  "engines": {
    "node": ">=0.8"
  }
}
